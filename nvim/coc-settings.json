{
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod", ".git/"],
			"filetypes": ["go"]
		},
		"ccls": {
			"command": "ccls",
			"filetypes": ["c", "cpp"],
			"rootPatterns": [".ccls", "compile_commands.json", ".git/"],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls"
				}
			}
		},
		"kotlin": {
            "command": "/home/gafeol/.config/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
            "filetypes": ["kotlin"]
        }
	},
	"suggest.removeDuplicateItems": true
}

